	<declaration>// Place global declarations here.
const int AgentNum = 3;
typedef int[0,AgentNum-1] AgentScale;
const int TaskNum = 4;
const int EventNum = 0;
const int MilestoneNum = 6;
const int MaxIteration = 1;

bool position[AgentNum][MilestoneNum]={{false,false,false,false,false,false},{false,false,false,false,false,false},{false,false,false,false,false,false}};
bool tf[AgentNum][TaskNum]={{true,false,false,false},{true,false,false,false},{true,false,false,false}};
bool ts[AgentNum][TaskNum]={{true,false,false,false},{true,false,false,false},{true,false,false,false}};
const int rules[AgentNum][MilestoneNum] = {{0,3,1,1,2,2},{0,3,1,1,2,2},{0,3,1,1,2,2}};

const int RegularTaskNum[AgentNum] = {3,3,3};
chan move[AgentNum], initialize, charge[AgentNum], exe[AgentNum], arrive[AgentNum];

//int iteration[AgentNum] = {0,0,0,0,0,0};
int stone = 90;
const int load = 15;
int battery[AgentNum] =  {2000, 2000, 2000};
const int consumingRate[AgentNum] = {1, 1, 1};
const int fullfil[AgentNum] = {500, 500, 500}; 

int PCQueue = 0;
clock globalTime;
const int MAXTIME = 3000;

bool jobsDone()
{
    return stone == 0;
}

bool enoughBattery(int agentID)
{
    return battery[agentID] &gt; 0;
}

bool shouldGo(int agentID, int currentPosition, int targetPosition)
{
    int currentTask = rules[agentID][currentPosition];
    int targetTask = rules[agentID][targetPosition];
    bool currentFinished = tf[agentID][currentTask];
    bool targetFinished = tf[agentID][targetTask];
    bool go = currentFinished &amp;&amp; (!targetFinished);

    return go;
}

</declaration>
	<template>
		<name>TaskExecution</name>
		<parameter>const AgentScale id</parameter>
		<declaration>clock t;

void refresh()
{
    battery[id] += fullfil[id];
}

void consume(int consumption)
{
    battery[id] -= consumption * consumingRate[id];
}

void updateIteration()
{
	int i=0;

	if(tf[id][3])
	{
		for(i=1;i&lt;=RegularTaskNum[id]-1;i++)
		{
			tf[id][i]=false;
		}
        stone = stone - load;
        if(stone &lt;= 0)
        {
            stone = 0;
        }
	}
	else
	{
    	//
	}
}

bool isBusy(int taskID)
{
    bool busy = false;
    int other_id = 0, other_position = -1, position_id = 0;

    if(jobsDone()) return true;

    for(other_id = 0; other_id &lt; AgentNum; other_id++)
    {
        if(other_id != id &amp;&amp; ts[other_id][taskID])
        {
            for(position_id = 0; position_id &lt; MilestoneNum; position_id++)
            {
                if(position[other_id][position_id])
                {
                    other_position = position_id;
                }
            }
            if(position[id][other_position])
            {
                busy = true;
            }
            else
            {
                busy = false;
            }

            return busy;
        }
    }

    return busy;
}



